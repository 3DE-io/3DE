.editors
  {{#section}}
  settings(tabs='{{code}}', selected='{{selected}}', error='{{error.location}}')
  .editor-container
    {{#code:language}}
    .editor(style="{{selected!==language ? 'visibility: none; z-index: -1;' : ''}}")
      editor(language='{{language}}', code='{{.}}', config='{{config[title][language]}}')
    {{/code}}
  error
  {{/section}}